<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/reservation_list_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/_2383C6">

    <View
        android:id="@+id/vArrowRight"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/arrow_right"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <View
        android:id="@+id/vArrowLeft"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/arrow_left"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:visibility="gone"
        />
    <TextView
        android:id="@+id/tvDeskNum"
        android:layout_width="155dp"
        android:layout_height="155dp"
        android:layout_marginStart="516dp"
        android:layout_marginTop="282dp"
        android:background="@drawable/rectangle_round_blue3"
        tools:text="00"
        android:textColor="@color/white"
        android:textSize="100sp"
        android:gravity="center"
        android:fontFamily="@font/custom_font_jalnan"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <TextView
        android:id="@+id/tvDeskName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        tools:text="간편상담"
        android:textColor="@color/_2383C6"
        android:textSize="116sp"
        android:gravity="center"
        android:fontFamily="@font/custom_font_pretendard_b"
        app:layout_constraintTop_toTopOf="@id/tvDeskNum"
        app:layout_constraintStart_toEndOf="@id/tvDeskNum"
        app:layout_constraintBottom_toBottomOf="@id/tvDeskNum"
        />
    <TextView
        android:id="@+id/tvCallNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="0000"
        android:textColor="@color/black"
        android:textSize="330sp"
        android:gravity="center"
        android:fontFamily="@font/custom_font_jalnan"
        app:layout_constraintTop_toBottomOf="@id/tvDeskNum"
        app:layout_constraintStart_toStartOf="@id/tvDeskNum"
        />
</androidx.constraintlayout.widget.ConstraintLayout>
