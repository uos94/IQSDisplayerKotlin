<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingVertical="20dp"
    tools:background="@color/FF0471E8"
    tools:context=".ui.MainActivity">

    <TextView
        android:id="@+id/tvWinNum"
        style="@style/CallItemWinNumStyle"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tvCallNum"
        tools:text="1"/>

    <TextView
        android:id="@+id/tvCallNum"
        style="@style/CallItemCallNumStyle"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvWinNum"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="1000"/>

</androidx.constraintlayout.widget.ConstraintLayout>