<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- FontWeight에 따른 용도
100    Extra Light or Ultra Light
200    Light or Thin
300    Book or Demi
400    Normal or Regular
500    Medium
600    Semibold, Demibold
700    Bold
800    Black, Extra Bold or Heavy
900    Extra Black, Fat, Poster or Ultra Black
    -->
    <!-- API 26 이상 android:fontStyle
         API 26 이하 app:fontStyle -->
    <font
        app:fontStyle="normal"
        app:fontWeight="200"
        app:font="@font/wooridauml"/>

    <font
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/wooridaumr"/>
    <font
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/wooridaumb"/>
</font-family>